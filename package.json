{
  "name": "n8n-nodes-docx-to-markdown",
  "version": "0.1.3",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Stephane Coupleux",
    "email": "stephane.coupleux@gmail.com"
  },
  "description": "n8n nodes to convert docx files to markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/scoupleuxSG/n8n-node-docx-to-markdown-new"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/DocxToMarkdown/DocxToMarkdown.node.js",
      "dist/nodes/HtmlToMarkdown/HtmlToMarkdown.node.js"
    ]
  },
  "dependencies": {
    "jsdom": "^27.0.0",
    "mammoth": "1.11.0",
    "sanitize-html": "^2.17.0",
    "turndown": "7.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.36.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "24.5.2",
    "@types/sanitize-html": "^2.16.0",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "9.36.0",
    "eslint-plugin-n8n-nodes-base": "1.16.3",
    "globals": "16.4.0",
    "gulp": "5.0.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-core": "1.112.1",
    "n8n-workflow": "1.110.0"
  }
}
